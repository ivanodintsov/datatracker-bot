{
  "extends": "eslint:recommended",
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2018
  },
  "env": {
    "node": true,
    "es6": true
  },
  "rules": {
    "no-debugger": 0,
    "no-console": 0,
    "new-cap": 0,
    "semi": 2,
    "strict": 0,
    "no-undef": 2,
    "no-underscore-dangle": 0,
    "no-use-before-define": 0,
    "eol-last": 0,
    "quotes": [ 2, "single" ],
    "jsx-quotes": [ 2, "prefer-single" ],
    "react/jsx-no-undef": 1,
    "react/jsx-uses-react": 1,
    "react/jsx-uses-vars": 1,
    "no-unused-vars": [ 2, { "vars": "all", "varsIgnorePattern": "[sS]tyles$" } ]
  }
}
